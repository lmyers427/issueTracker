      <!-- Will finish this section once table page is generated -->
          <div class="table-responsive">
                  <table class="table align-items-center table-flush table-borderless">
                   <thead>
                    <tr>
                      <th>Ticket ID</th>
                      <th>Title</th>
                      <th>Category</th>
                      <th>Description</th>
                      <th>Prioity</th>
                      <th>Status</th>
                    </tr>
                    </thead>
       <% if(userTickets) { %>
       
       <% userTickets.forEach( function(property) { %>
 
                    <tbody><tr>
                     <td><%=property._id%></td>
                     <td><%=property.name%></td>
                     <td><%=property.category%></td>
                     <td><%=property.initialDescription%></td>
                     <td><%=property.priority%></td>
                     <td><%=property.status%></td>
                    </tr>

                    <% }) %>

                    <% } %>
 
                  </tbody></table>
                </div>
      </div>
    </div>
